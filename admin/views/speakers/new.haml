.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), url(:speakers, :index)
      %li.active=link_to pat(:new), url(:speakers, :new)
  .content
    %h2.title
      =pat(:new)
      =mt(:speaker)
    .inner
      -form_for :speaker, url(:speakers, :create), :class => :form do |f|
        =partial "speakers/form", :locals => { :f => f }      

-content_for :sidebar, partial("base/sidebar")
